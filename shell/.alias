# Author: Ryan Hendrickson
# Filename: .aliases
# Contains a bunch of aliases I use.

alias a='php artisan'
alias bg='batgrep'
alias bm='batman'
alias ciw='curl -iw "\nTime: %{time_total}s \n"'
alias code='codium'
alias com='composer'
alias con='php bin/console'
alias dots='vim $HOME/.dotfiles/'
alias ga='git add'
alias gch='git checkout'
alias gco='git commit -m'
alias gd='git diff'
alias gom='migrate create -seq -ext=.sql -dir=./migrations' # relies on https://github.com/golang-migrate/migrate
alias gp='git push'
alias gs='git status'
alias k='clear'
alias ll='eza -laghF --git --icons --color=always'
alias lla='ls -lhFA'
alias nvimconf='vim $HOME/.config/nvim/'
alias pla='cd $HOME/playground/'
alias pro='cd $HOME/projects/'
alias pu='./vendor/bin/phpunit'
alias puu='./vendor/bin/phpunit --testdox'
alias rgf='rg --files | rg'
alias sh-bash='sudo ln -sfv /bin/bash /private/var/select/sh'
alias sh-dash='sudo ln -sfv /bin/dash /private/var/select/sh'
alias spu='symfony php vendor/bin/phpunit'
alias spuu='symfony php vendor/bin/phpunit --textdox'
alias sm='symfony composer'
alias sn='symfony console'
alias sp='symfony php'
alias vim='nvim'
